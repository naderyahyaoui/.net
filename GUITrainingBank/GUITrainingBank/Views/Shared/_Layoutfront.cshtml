<!DOCTYPE html>
<html lang="en">
<head>
    @Scripts.Render("~/bundles/jsTheme")
    <!-- Basic Page Needs
    ================================================== -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>All Safe : Home</title>
   

    <!-- Mobile Specific Metas
    ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/png" href="@Url.Content("~/Content/frontoffice/")img/favicon.png" />

    <!-- CSS
    ================================================== -->
    <!-- Bootstrap css file-->
    <link href="@Url.Content("~/Content/frontoffice/")css/bootstrap.min.css" rel="stylesheet">
    <!-- Font awesome css file-->
    <link href="@Url.Content("~/Content/frontoffice/")css/font-awesome.min.css" rel="stylesheet">
    <!-- Superslide css file-->
    <link rel="stylesheet" href="@Url.Content("~/Content/frontoffice/")css/superslides.css">
    <!-- Slick slider css file -->
    <link href="@Url.Content("~/Content/frontoffice/")css/slick.css" rel="stylesheet">
    <!-- smooth animate css file -->
    <link rel="stylesheet" href="@Url.Content("~/Content/frontoffice/")css/animate.css">
    <!-- Elastic grid css file -->
    <link rel="stylesheet" href="@Url.Content("~/Content/frontoffice/")css/elastic_grid.css">
    <!-- Circle counter cdn css file -->
    <link rel='stylesheet prefetch' href='https://cdn.rawgit.com/pguso/jquery-plugin-circliful/master/css/jquery.circliful.css'>
    <!-- Default Theme css file -->
    <link id="orginal" href="@Url.Content("~/Content/frontoffice/")css/themes/default-theme.css" rel="stylesheet">
    <!-- Main structure css file -->
    <link href="@Url.Content("~/Content/frontoffice/")style.css" rel="stylesheet">

    <!-- Google fonts -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Varela' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    
    <!-- BEGAIN PRELOADER -->
    <div id="preloader">
        <div id="status">&nbsp;</div>
    </div>
    <!-- END PRELOADER -->
    <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-angle-up"></i></a>
    <!-- END SCROLL TOP BUTTON -->
    <!--=========== BEGIN HEADER SECTION ================-->
    <header id="header">
        <!-- BEGIN MENU -->
        <div class="menu_area">
            <nav class="navbar navbar-default navbar-fixed-top" role="navigation" style="
    margin-top: 0px;
    padding-top: 0px;
">
                <div class="container"style="background-color:#444;margin-left: 0px;margin-right: 0px;width: 1905px;padding-right: 40px;">
                    <div class="navbar-header">
                        <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>

                        <!-- LOGO -->
                        <!-- TEXT BASED LOGO -->
                        <a class="navbar-brand" href="#">All<span>Safe<img src="@Url.Content("~/Content/frontoffice/")img/Indexlogo.png" width="50px" height="40px" style="margin-top:-10px" /></span>
                       
                            </a>
                        

                        <!-- IMG BASED LOGO  -->
                        <!--  <a class="navbar-brand" href="#"><img src="img/logo.png" alt="logo"></a> -->


                    </div>
                    @RenderBody()</div>
    
    <!--=========== END SUBSCRIBE SECTION ================-->
    <!--=========== BEGAIN FOOTER ================-->
    <footer id="footer" style="position: fixed; bottom: 0">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="footer_left">
                        <!--=========== Designed By WpFreeware Team ================-->
                        <p>All Rights Reserved <a href="#">AllSafe</a>.</p>
                        <!--=========== Designed By WpFreeware Team ================-->
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="footer_right">
                        <ul class="social_nav">
                            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                            <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!--=========== END FOOTER ================-->
    <!-- Javascript Files
    ================================================== -->
    <!-- initialize jQuery Library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <!-- Google map -->
    <script src="https://maps.googleapis.com/maps/api/js"></script>
    <script src="@Url.Content("~/Content/frontoffice/")js/jquery.ui.map.js"></script>
    <!-- For smooth animatin  -->
    <script src="@Url.Content("~/Content/frontoffice/")js/wow.min.js"></script>
    <!-- Bootstrap js -->
    <script src="@Url.Content("~/Content/frontoffice/")js/bootstrap.min.js"></script>
    <!-- superslides slider -->
    <script src="@Url.Content("~/Content/frontoffice/")js/jquery.superslides.min.js" type="text/javascript"></script>
    <!-- slick slider -->
    <script src="@Url.Content("~/Content/frontoffice/")js/slick.min.js"></script>
    <!-- for circle counter -->
    <script src='https://cdn.rawgit.com/pguso/jquery-plugin-circliful/master/js/jquery.circliful.min.js'></script>
    <!-- for portfolio filter gallery -->
    <script src="@Url.Content("~/Content/frontoffice/")js/modernizr.custom.js"></script>
    <script src="@Url.Content("~/Content/frontoffice/")js/classie.js"></script>
    <script src="@Url.Content("~/Content/frontoffice/")js/elastic_grid.min.js"></script>
    <script src="@Url.Content("~/Content/frontoffice/")js/portfolio_slider.js"></script>

    <!-- Custom js-->
    <script src="@Url.Content("~/Content/frontoffice/")js/custom.js"></script>
    <script src="@Url.Content("~/Content/frontoffice/")js/echarts-all.js"></script>
    <script src="@Url.Content("~/Content/frontoffice/")js/green.js"></script>
    
</body>
</html>